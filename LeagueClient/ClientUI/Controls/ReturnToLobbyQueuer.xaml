<UserControl x:Class="LeagueClient.ClientUI.Controls.ReturnToLobbyQueuer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:local="clr-namespace:LeagueClient.ClientUI.Controls">
  <Grid Width="150" Height="40">
    <Grid.RowDefinitions>
      <RowDefinition/>
      <RowDefinition/>
    </Grid.RowDefinitions>
    <TextBlock Style="{StaticResource Control}" Margin="2">Currenty in lobby</TextBlock>
    <Button Grid.Row="1" Width="20" HorizontalAlignment="Left" Click="Home_Click" Style="{StaticResource HomeButton}"/>
    <Button Grid.Row="1" HorizontalAlignment="Right" Click="Close_Click" Style="{StaticResource CloseButton}"/>
  </Grid>
</UserControl>
