<UserControl x:Class="LeagueClient.ClientUI.Controls.TeambuilderSoloQueuer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006">
  <Grid Width="150" Height="40">
    <Grid.RowDefinitions>
      <RowDefinition/>
      <RowDefinition/>
    </Grid.RowDefinitions>
    <TextBlock Grid.Row="0" VerticalAlignment="Center" Margin="3 0">Searching for Group...</TextBlock>
    <TextBlock Grid.Row="1" VerticalAlignment="Center" Margin="3 0">Time Elapsed:</TextBlock>
    <TextBlock Grid.Row="1" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="3 0"
               Name="ElapsedText"/>
  </Grid>
</UserControl>
